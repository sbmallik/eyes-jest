{
  "env": {
    "browser": true,
    "jest": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 9
  },
  "root": true,
  "rules": {
    "eqeqeq": 2,
    "no-compare-neg-zero": 2,
    "no-console": 1,
    "no-debugger": 1,
    "no-dupe-args": 2,
    "no-extra-parens": 1,
    "no-extra-semi": 2,
    "no-invalid-regexp": 2,
    "no-irregular-whitespace": 2,
    "no-misleading-character-class": 1,
    "no-unexpected-multiline": 2,
    "no-unreachable": 2,
    "no-unsafe-finally": 2,
    "no-unsafe-negation": 2,
    "use-isnan": 2,
    "valid-jsdoc": [2, {
      "requireParamDescription": false
    }],
    "valid-typeof": 2,
    "dot-location": [2, "property"],
    "dot-notation": 2,
    "guard-for-in": 2,
    "no-alert": 2,
    "no-caller": 1,
    "no-div-regex": 2,
    "no-else-return": 1,
    "no-empty-pattern": 2,
    "no-eq-null": 2,
    "no-eval": 2,
    "no-extend-native": 2,
    "no-extra-bind": 2,
    "no-extra-label": 2,
    "no-global-assign": 2,
    "no-implied-eval": 2,
    "no-iterator": 2,
    "no-lone-blocks": 2,
    "no-multi-spaces": [2, {
      "exceptions": {
        "Property": true,
        "VariableDeclarator": true
      }
    }],
    "no-new-func": 2,
    "no-new-wrappers": 2,
    "no-octal": 2,
    "no-octal-escape": 2,
    "no-redeclare": 2,
    "no-return-await": 2,
    "no-script-url": 2,
    "no-self-assign": 2,
    "no-self-compare": 2,
    "no-throw-literal": 2,
    "no-unmodified-loop-condition": 2,
    "no-unused-expressions": [2, {
      "allowShortCircuit": true,
      "allowTernary": true
      }],
    "no-unused-labels": 2,
    "no-useless-call": 2,
    "no-useless-concat": 2,
    "no-useless-escape": 2,
    "no-useless-return": 2,
    "no-warning-comments": [1, {
      "terms": ["todo"],
      "location": "start"
    }],
    "no-with": 2,
    "prefer-promise-reject-errors": 2,
    "radix": [2, "as-needed"],
    "require-await": 2,
    "wrap-iife": [2, "inside"],
    "yoda": 2,
    "no-delete-var": 2,
    "no-label-var": 2,
    "no-restricted-globals": ["error", "event"],
    "no-shadow-restricted-names": 2,
    "no-undef-init": 2,
    "no-undefined": 2,
    "no-unused-vars": 2,
    "array-bracket-spacing": [2, "never"],
    "brace-style": 2,
    "camelcase": 2,
    "comma-dangle": [2, "never"],
    "comma-spacing": 2,
    "comma-style": 2,
    "computed-property-spacing": 2,
    "eol-last": 2,
    "func-call-spacing": 2,
    "func-names": [2, "as-needed"],
    "func-style": [2, "declaration"],
    "function-paren-newline": [2, "never"],
    "implicit-arrow-linebreak": [2, "beside"],
    "indent": [2, 2, {
      "SwitchCase": 1,
      "VariableDeclarator": {
        "var": 2,
        "let": 2,
        "const": 3
      },
      "FunctionDeclaration": {"parameters": "first"},
      "CallExpression": {"arguments": "first"},
      "ignoredNodes": ["ConditionalExpression"]
    }],
    "key-spacing": 2,
    "keyword-spacing": 2,
    "linebreak-style": 2,
    "multiline-ternary": [2, "never"],
    "no-array-constructor": 2,
    "no-lonely-if": 2,
    "no-mixed-spaces-and-tabs": 2,
    "no-negated-condition": 2,
    "no-new-object": 2,
    "no-tabs": 2,
    "no-trailing-spaces": [2, {
      "skipBlankLines": true
    }],
    "no-underscore-dangle": 2,
    "no-unneeded-ternary": 2,
    "no-var": 2,
    "no-whitespace-before-property": 2,
    "nonblock-statement-body-position": 2,
    "object-curly-newline": [2, {
      "ObjectExpression": {
        "minProperties": 1
      },
      "ObjectPattern": {
        "multiline": true
      }
    }],
    "object-curly-spacing": 2,
    "object-property-newline": 2,
    "one-var": [2, "consecutive"],
    "one-var-declaration-per-line": 2,
    "operator-assignment": 2,
    "operator-linebreak": [2, "after"],
    "quote-props": [2, "as-needed"],
    "quotes": [2, "single", {
      "avoidEscape": true
    }],
    "semi": 2,
    "semi-spacing": 2,
    "semi-style": 2,
    "space-before-blocks": 2,
    "space-before-function-paren": [2, "never"],
    "space-in-parens": 2,
    "space-infix-ops": 2,
    "space-unary-ops": [2, {
      "words": true,
      "nonwords": false
    }],
    "spaced-comment": 2,
    "switch-colon-spacing": 2,
    "template-tag-spacing": 2,
    "unicode-bom": 2,
    "arrow-body-style": [2, "always"],
    "arrow-parens": 2,
    "arrow-spacing": 2,
    "constructor-super": 2,
    "generator-star-spacing": [2, {
      "before": false,
      "after": true
    }],
    "no-confusing-arrow": [2, {
      "allowParens": true
    }],
    "no-const-assign": 2,
    "no-dupe-class-members": 2,
    "no-duplicate-imports": 2,
    "no-new-symbol": 2,
    "no-this-before-super": 2,
    "no-useless-computed-key": 2,
    "no-useless-constructor": 2,
    "no-useless-rename": 2,
    "prefer-numeric-literals": 1,
    "require-yield": 2,
    "rest-spread-spacing": 2,
    "symbol-description": 2,
    "template-curly-spacing": 2,
    "yield-star-spacing": 2
  }
}
