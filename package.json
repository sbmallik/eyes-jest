{
  "name": "eyes-jest",
  "description": "Applitools visual testing with JEST and Sauce Labs",
  "version": "0.1.0",
  "private": false,
  "license": "ISC",
  "keywords": [
    "jest",
    "saucelabs",
    "applitools",
    "javascript",
    "selenium",
    "webdriver"
  ],
  "bugs": "https://github.com/sbmallik/eyes-jest/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/sbmallik/eyes-jest"
  },
  "scripts": {
    "test": "jest --detectOpenHandles --no-cache"
  },
  "jest": {
    "extraGlobals": [
      "Math"
    ],
    "testEnvironment": "jest-environment-node",
    "testMatch": [
      "**/test/**/*.js"
    ]
  },
  "dependencies": {
    "@applitools/eyes-common": "^1.2.2",
    "@applitools/eyes-sdk-core": "^4.9.1",
    "@applitools/eyes-selenium": "^4.7.0"
  },
  "devDependencies": {
    "chromedriver": "^2.45.0",
    "jest": "^24.0.0",
    "jest-environment-node": "^23.1.0",
    "selenium-webdriver": "^4.0.0-alpha.1",
    "saucelabs": "^1.5.0"
  },
  "engines": {
    "node": ">=8.9.0"
  }
}
